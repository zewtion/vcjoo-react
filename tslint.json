{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "no-empty": [false, "allow-empty-catch", "allow-empty-functions"],
  "curly": [true, "as-needed"],
  "noUnusedLocals":[true],
  "array-type": [true, "generic"]
}
